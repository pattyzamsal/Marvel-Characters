# Uncomment the next line to define a global platform for your project
platform :ios, '13.0'

project 'MarvelCharacters.xcodeproj'
use_frameworks!

def include_pods
  pod 'Alamofire', '~> 5.2'
  pod 'AlamofireImage', '~> 4.1'

end

target 'Data' do
  include_pods
end


target 'MarvelCharacters' do
  include_pods
end